<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0" />
  <template class="PawfectlyHeaderUi" parent="GtkBox">
    <property name="width-request">300</property>
    <property name="height-request">120</property>
    <property name="name">header</property>
    <child>
      <object class="GtkBox" id="head">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel">
            <property name="label">PawFectly</property>
            <property name="name">pawfectly</property>
            <property name="height-request">80</property>
            <property name="margin-start">50</property>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="namebar">
            <property name="name">namebar</property>
            <property name="orientation">horizontal</property>
            <property name="margin_start">50</property>
            <child>
              <object class="GtkButton" id="signin">
                <property name="name">sign</property>
                <property name="label">SignIn</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="signup">
                <property name="margin_start">50</property>
                <property name="name">sign</property>
                <property name="label">SignUp</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>