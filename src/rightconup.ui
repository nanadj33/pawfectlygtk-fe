<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0" />
  <template class="PawfectlyRightconupUi" parent="GtkBox">
    <child>
      <object class="GtkBox" id="rightconup">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox">
            <property name="name">header</property>
            <property name="margin-start">20</property>
            <property name="orientation">vertical</property>
            <child>
              <object class="GtkBox">
                <child>
                  <object class="GtkImage">
                    <property name="margin-start">30</property>
                    <property name="resource">/org/example/App/assets/logopaw.svg</property>
                    <property name="pixel-size">45</property>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel">
                    <property name="margin-start">10</property>
                    <property name="label">PawFectly</property>
                    <property name="name">pawfectly</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="namebar">
                <property name="margin-start">35</property>
                <property name="orientation">horizontal</property>
                <property name="margin_top">5</property>
                <child>
                  <object class="GtkButton" id="signin">
                    <property name="name">sign</property>
                    <property name="label">SignIn</property>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="signup">
                    <property name="margin_start">30</property>
                    <property name="name">signi</property>
                    <property name="label">SignUp</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="content">
            <property name="orientation">vertical</property>
            <property name="margin_start">20</property>
            <child>
              <object class="GtkEntry" id="email">
                <property name="name">inputbar</property>
                <property name="margin_top">20</property>
                <property name="placeholder-text">E-mail</property>
                <property name="width-request">280</property>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="uname">
                <property name="name">inputbar</property>
                <property name="margin_top">10</property>
                <property name="placeholder-text">Username</property>
                <property name="width-request">280</property>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="fullname">
                <property name="name">inputbar</property>
                <property name="margin_top">10</property>
                <property name="placeholder-text">Full Name</property>
                <property name="width-request">280</property>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="phone">
                <property name="name">inputbar</property>
                <property name="margin_top">10</property>
                <property name="placeholder-text">Phone Number</property>
                <property name="width-request">280</property>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="pass">
                <property name="name">inputbar</property>
                <property name="margin_top">10</property>
                <property name="placeholder-text">Password</property>
                <property name="visibility">False</property>
                <property name="width-request">280</property>
              </object>
            </child>
            <child>
              <object class="GtkEntry" id="conpass">
                <property name="name">inputbar</property>
                <property name="margin_top">10</property>
                <property name="placeholder-text">Confirm Password</property>
                <property name="visibility">False</property>
                <property name="width-request">280</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="regis">
                <property name="label">Sign me up!</property>
                <property name="name">signin</property>
                <property name="margin-top">5</property>
                <property name="margin-start">80</property>
                <property name="margin-end">80</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>